{
  "name": "yukid",
  "version": "0.1.0",
  "description": "Aims to provide effortless management of docker containers on USTC Mirrors",
  "main": "dist/index.js",
  "repository": "https://github.com/knight42/Yuki/tree/master/packages/yukid",
  "scripts": {
    "test": "NODE_ENV=test ava --no-cache",
    "dev": "babel -wd dist src",
    "pub": "npm run build && npm publish",
    "lint": "eslint src",
    "doc": "apidoc -i src/routes -f api.js",
    "build": "NODE_ENV=production babel -d dist src",
    "build:dev": "babel -d dist src",
    "yukid": "NODE_ENV=production dist/index.js",
    "yukid:dev": "NODE_ENV=dev dist/index.js",
    "yukid:test": "NODE_ENV=test dist/index.js"
  },
  "author": "knight_42 <anonymousknight96@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "babel-runtime": "^6.22.0",
    "bluebird": "^3.5.0",
    "dockerode": "^2.5.0",
    "is-listening": "^1.0.1",
    "koa": "^2.3.0",
    "koa-bodyparser": "=3.2.0",
    "koa-router": "^7.2.1",
    "moment": "^2.18.1",
    "mongoose": "^4.11.1",
    "node-pty": "^0.6.9",
    "node-schedule": "^1.2.3",
    "socket.io": "^2.0.3",
    "split": "^1.0.0",
    "winston": "^2.3.1"
  },
  "files": [
    "dist/",
    "src/"
  ],
  "preferGlobal": true,
  "bin": {
    "yukid": "dist/index.js"
  },
  "engines": {
    "node": ">=6"
  },
  "devDependencies": {
    "ava": "^0.20.0",
    "babel-cli": "^6.24.0",
    "babel-eslint": "^7.2.3",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-preset-babili": "^0.0.12",
    "babel-preset-env": "^1.6.0",
    "babel-register": "^6.24.0",
    "eslint": "^4.2.0",
    "eslint-plugin-import": "^2.7.0",
    "node-fetch": "^1.7.1"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "node": 6
        }
      ]
    ],
    "env": {
      "production": {
        "presets": [
          "babili"
        ]
      }
    },
    "plugins": [
      "add-module-exports",
      [
        "transform-runtime",
        {
          "polyfill": false,
          "regenerator": true
        }
      ]
    ]
  },
  "ava": {
    "require": [
      "babel-register"
    ],
    "babel": "inherit",
    "failFast": true,
    "verbose": true,
    "files": [
      "test/*.js"
    ]
  }
}

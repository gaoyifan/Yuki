[{
    "name": "archlinux",
    "interval": "* 1 * 2 3 4",
    "image": "ubuntu:latest",
    "command": ["sleep", "5"],
    "envs": ["RSYNC_USER=asdh"],
    "volumes": ["/arch:/srv/repo/arch"],
    "user": "mirror:mirror"
},
{
    "name": "ubuntu",
    "interval": "* 1 * 2 3 4",
    "image": "ustclug/alpine:latest",
    "command": ["sh"],
    "envs": ["RSYNC_PASS=asdh"],
    "volumes": ["/ubuntu:/srv/repo/ubuntu"],
    "user": "1000:1001"
},
{
    "name": "gmt",
    "interval": "* 1 * 2 3 4",
    "image": "debian:latest",
    "command": ["sleep", "5"],
    "envs": ["RSYNC_PASS=asdh"],
    "volumes": ["/gmt:/srv/repo/gmt"],
    "user": "1000:1001"
},
{
    "name": "pypi",
    "interval": "* 1 * 2 3 4",
    "image": "ustclug/alpine:latest",
    "command": ["sleep", "5"],
    "envs": ["RSYNC_PASS=asdh"],
    "volumes": ["/pypi:/srv/repo/pypi"],
    "user": ""
}
]
